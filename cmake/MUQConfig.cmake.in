# - Config file for the MUQ package
# It defines the following variables
#  MUQ_INCLUDE_DIRS - include directories for MUQ
#  MUQ_LIBRARIES    - MUQ libraries to link against
#  MUQ_LINK_LIBRARIES - libraries MUQ links against
#  MUQ_INCLUDE_DIRS   - Include directories that MUQ uses
#  MUQ_COMPILE_DEFINITIONS -Definitions used during compilation (i.e. variables added with add_definitions)

set(@CMAKE_PROJECT_NAME@_INCLUDE_DIRS @CMAKE_INSTALL_PREFIX@/include @MUQ_EXTERNAL_INCLUDES@)

set(@CMAKE_PROJECT_NAME@_LIBRARY_DIR @CMAKE_INSTALL_PREFIX@/lib)
set(@CMAKE_PROJECT_NAME@_EXTERNAL_LIBRARY_DIR @CMAKE_INSTALL_PREFIX@/muq_external/lib)

# These are IMPORTED targets created by MUQDepends.cmake
set(@CMAKE_PROJECT_NAME@_LIBRARIES @MUQ_EXPORT_LIBRARIES@)
set(@CMAKE_PROJECT_NAME@_LINK_LIBRARIES @MUQ_LINK_LIBS@)
set(@CMAKE_PROJECT_NAME@_LINK_LIBRARIES_STATIC @MUQ_LINK_LIBS_STATIC@)

# These are the compiler flags used to compile MUQ
set(@CMAKE_PROJECT_NAME@_CXX_FLAGS "@CMAKE_CXX_FLAGS@")
set(@CMAKE_PROJECT_NAME@_C_FLAGS "@CMAKE_C_FLAGS@")
set(@CMAKE_PROJECT_NAME@_CXX_COMPILER "@CMAKE_CXX_COMPILER@")
set(@CMAKE_PROJECT_NAME@_C_COMPILER "@CMAKE_C_COMPILER@")

set(@CMAKE_PROJECT_NAME@_COMPILE_DEFINITIONS "@MUQ_COMPILE_DEFINITIONS@")
